{
  "name": "unplugins-monorepo",
  "version": "1.0.0",
  "private": true,
  "workspaces": [
    "packages/*",
    "clients/*"
  ],
  "type": "module",
  "scripts": {
    "types": "pnpm run -r types",
    "build": "pnpm run -r build",
    "release": "pnpm types && pnpm build && pnpm publish -r",
    "lint": "eslint . --fix",
    "prepare": "husky install",
    "web": "pnpm --filter vite-app dev"
  },
  "repository": "git@github.com:gympass/unplugins.git",
  "author": "Kaic Bastidas <kaic.bastidas@gympass.com>",
  "license": "MIT",
  "devDependencies": {
    "@changesets/cli": "^2.24.4",
    "@commitlint/cli": "^17.1.2",
    "@commitlint/config-conventional": "^17.1.0",
    "@react-native-community/eslint-config": "^3.1.0",
    "@typescript-eslint/eslint-plugin": "^5.39.0",
    "@typescript-eslint/parser": "^5.39.0",
    "esbuild": "^0.15.14",
    "eslint": "^8.24.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-prettier": "^4.2.1",
    "happy-dom": "^7.7.0",
    "husky": "^8.0.0",
    "prettier": "^2.7.1",
    "tiny-glob": "^0.2.9",
    "typescript": "^4.9.3",
    "vitest": "^0.25.2"
  }
}
