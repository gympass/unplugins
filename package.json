{
  "name": "unplugins-monorepo",
  "version": "1.0.0",
  "private": true,
  "workspaces": [
    "packages/*",
    "clients/*"
  ],
  "type": "module",
  "scripts": {
    "types": "pnpm run -r types",
    "build": "pnpm run -r build",
    "release": "pnpm types && pnpm build && pnpm publish -r",
    "lint": "eslint . --fix",
    "prepare": "husky install",
    "web": "pnpm --filter vite-web-app dev",
    "dev:graphql-parse": "pnpm --filter unplugin-graphql-parse dev"
  },
  "repository": "git@github.com:gympass/unplugins.git",
  "author": "Kaic Bastidas <kaic.bastidas@gympass.com>",
  "license": "MIT",
  "devDependencies": {
    "@changesets/cli": "^2.26.1",
    "@commitlint/cli": "^17.5.1",
    "@commitlint/config-conventional": "^17.4.4",
    "@react-native-community/eslint-config": "^3.2.0",
    "@typescript-eslint/eslint-plugin": "^5.57.1",
    "@typescript-eslint/parser": "^5.57.1",
    "esbuild": "^0.17.15",
    "eslint": "^8.37.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-prettier": "^4.2.1",
    "happy-dom": "^7.8.1",
    "husky": "^8.0.3",
    "prettier": "^2.8.7",
    "typescript": "^4.9.5",
    "vitest": "^0.25.8"
  }
}
